#include "source/Core/MathFunctionsImpl.h"
#include "source/Core/arch/Default/ConjHelper.h"
// Generic half float support
#include "source/Core/arch/Default/Half.h"
#include "source/Core/arch/Default/BFloat16.h"
#include "source/Core/arch/Default/TypeCasting.h"
#include "source/Core/arch/Default/GenericPacketMathFunctionsFwd.h"

#if defined EIGEN_VECTORIZE_AVX512
  #include "source/Core/arch/SSE/PacketMath.h"
  #include "source/Core/arch/SSE/TypeCasting.h"
  #include "source/Core/arch/SSE/Complex.h"
  #include "source/Core/arch/AVX/PacketMath.h"
  #include "source/Core/arch/AVX/TypeCasting.h"
  #include "source/Core/arch/AVX/Complex.h"
  #include "source/Core/arch/AVX512/PacketMath.h"
  #include "source/Core/arch/AVX512/TypeCasting.h"
  #include "source/Core/arch/AVX512/Complex.h"
  #include "source/Core/arch/SSE/MathFunctions.h"
  #include "source/Core/arch/AVX/MathFunctions.h"
  #include "source/Core/arch/AVX512/MathFunctions.h"
#elif defined EIGEN_VECTORIZE_AVX
  // Use AVX for floats and doubles, SSE for integers
  #include "source/Core/arch/SSE/PacketMath.h"
  #include "source/Core/arch/SSE/TypeCasting.h"
  #include "source/Core/arch/SSE/Complex.h"
  #include "source/Core/arch/AVX/PacketMath.h"
  #include "source/Core/arch/AVX/TypeCasting.h"
  #include "source/Core/arch/AVX/Complex.h"
  #include "source/Core/arch/SSE/MathFunctions.h"
  #include "source/Core/arch/AVX/MathFunctions.h"
#elif defined EIGEN_VECTORIZE_SSE
  #include "source/Core/arch/SSE/PacketMath.h"
  #include "source/Core/arch/SSE/TypeCasting.h"
  #include "source/Core/arch/SSE/MathFunctions.h"
  #include "source/Core/arch/SSE/Complex.h"
#elif defined(EIGEN_VECTORIZE_ALTIVEC) || defined(EIGEN_VECTORIZE_VSX)
  #include "source/Core/arch/AltiVec/PacketMath.h"
  #include "source/Core/arch/AltiVec/MathFunctions.h"
  #include "source/Core/arch/AltiVec/Complex.h"
#elif defined EIGEN_VECTORIZE_NEON
  #include "source/Core/arch/NEON/PacketMath.h"
  #include "source/Core/arch/NEON/TypeCasting.h"
  #include "source/Core/arch/NEON/MathFunctions.h"
  #include "source/Core/arch/NEON/Complex.h"
#elif defined EIGEN_VECTORIZE_SVE
  #include "source/Core/arch/SVE/PacketMath.h"
  #include "source/Core/arch/SVE/TypeCasting.h"
  #include "source/Core/arch/SVE/MathFunctions.h"
#elif defined EIGEN_VECTORIZE_ZVECTOR
  #include "source/Core/arch/ZVector/PacketMath.h"
  #include "source/Core/arch/ZVector/MathFunctions.h"
  #include "source/Core/arch/ZVector/Complex.h"
#elif defined EIGEN_VECTORIZE_MSA
  #include "source/Core/arch/MSA/PacketMath.h"
  #include "source/Core/arch/MSA/MathFunctions.h"
  #include "source/Core/arch/MSA/Complex.h"
#endif

#if defined EIGEN_VECTORIZE_GPU
  #include "source/Core/arch/GPU/PacketMath.h"
  #include "source/Core/arch/GPU/MathFunctions.h"
  #include "source/Core/arch/GPU/TypeCasting.h"
#endif

#if defined(EIGEN_USE_SYCL)
  #include "source/Core/arch/SYCL/SyclMemoryModel.h"
  #include "source/Core/arch/SYCL/InteropHeaders.h"
#if !defined(EIGEN_DONT_VECTORIZE_SYCL)
  #include "source/Core/arch/SYCL/PacketMath.h"
  #include "source/Core/arch/SYCL/MathFunctions.h"
  #include "source/Core/arch/SYCL/TypeCasting.h"
#endif
#endif

#include "source/Core/arch/Default/Settings.h"
// This file provides generic implementations valid for scalar as well
#include "source/Core/arch/Default/GenericPacketMathFunctions.h"

#include "source/Core/functors/TernaryFunctors.h"
